{
  "name": "react-model",
  "version": "0.1.1",
  "description": "The missing Model for React.js",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6",
    "build-cjs": "rimraf lib && babel ./src -d lib --ignore '__tests__'",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules --ignore '__tests__'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bbtfr/react-model.git"
  },
  "keywords": [
    "react",
    "model",
    "collection"
  ],
  "author": "Theo Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbtfr/react-model/issues"
  },
  "homepage": "https://github.com/bbtfr/react-model#readme",
  "dependencies": {
    "lodash": "^4.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.3.26",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "rimraf": "^2.5.2"
  }
}
